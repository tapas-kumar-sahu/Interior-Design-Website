{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "lib": [
            "esnext"
        ],
        "skipLibCheck": true,
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "baseUrl": "..",
        "paths": {
            "@/*": [
                "./*"
            ]
        }
    }
}